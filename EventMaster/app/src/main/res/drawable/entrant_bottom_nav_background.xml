<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/entrant_glow" />
            <corners android:radius="18dp" />
        </shape>
    </item>
    <item android:left="1dp" android:right="1dp" android:top="1dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/entrant_surface" />
            <corners android:radius="18dp" />
            <stroke android:width="1dp" android:color="@color/entrant_outline" />
        </shape>
    </item>
</layer-list>
